---
import SimonSaysVue from "./SimonSaysVue.vue";
let { title } = Astro.props;
import { getEntryBySlug } from "astro:content";
if (!title)
  title = (await getEntryBySlug("docs", Astro.params.slug as string))?.data
    .description;
---

<SimonSaysVue client:only="vue" title={title}>
  <slot />
</SimonSaysVue>
